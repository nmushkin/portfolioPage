{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "entries",
  "info": {
    "name": "Entry"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Entry_body": {
      "type": "richtext"
    },
    "written_date": {
      "type": "date"
    },
    "entry_pics": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "summary": {
      "type": "string"
    }
  }
}
